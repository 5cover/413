{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch built-in server and debug",
      "type": "php",
      "request": "launch",
      "cwd": "${workspaceFolder}/html",
      "runtimeArgs": [
        "-S",
        "localhost:8000",
        "-d",
        "include_path=${workspaceFolder}/include",
        "-c",
        "${workspaceFolder}/cfg/php.ini"
      ],
      "port": 9003,
      "serverReadyAction": {
        "action": "openExternally"
      }
    },
    {
      "name": "Debug testing script",
      "type": "php",
      "request": "launch",
      "program": "${file}",
      "runtimeArgs": [
        "-c",
        "${workspaceFolder}/tests/php.ini"
      ],
      "cwd": "${fileDirname}",
      "externalConsole": false,
      "port": 9003
    },
    {
      "name": "Listen for Xdebug",
      "type": "php",
      "request": "launch",
      "port": 9003
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Users/benja/OneDrive/Documents/cours/A2/SAE3/gitLocal/LHostManager",
      "program": "c:/Users/benja/OneDrive/Documents/cours/A2/SAE3/gitLocal/LHostManager/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}